/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "types": [
      "node"
    ],
    "strict": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "es2020",
    "paths": {
      "@core/*": [
        "./src/app/core/*"
      ],
      "@modules/*": [
        "./src/app/modules/*"
      ],
      "@shared/*": [
        "./src/app/shared/*"
      ],
      "@store/*": [
        "./src/app/core/store/*"
      ],
      "@environments/*": [
        "./src/environments/*"
      ]
    },
    "lib": [
      "es2018",
      "dom"
    ],
    "useDefineForClassFields": false
  }
}
